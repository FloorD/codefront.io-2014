</div>


<div id="tickets" class="block">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h1>Tickets</h1>
            </div>
        </div>

        <%= partial "partials/tickets" %>
    </div>
</div>

<div id="venue" class="block">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h1>Venue</h1>
            </div>
        </div>
        <%= partial "partials/venue" %>
    </div>
</div>


<div id="sponsors" class="block">
    <div class="container">
        <div class="row">
